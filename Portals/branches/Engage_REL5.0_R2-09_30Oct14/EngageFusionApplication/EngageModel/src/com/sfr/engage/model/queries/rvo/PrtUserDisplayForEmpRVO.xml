<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PrtUserDisplayForEmpRVO"
  Version="11.1.1.61.92"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.sfr.engage.model.queries.rvo.PrtUserDisplayForEmpRVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="cc"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="PID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="roleName"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT ROLEMAP.USER_EMAIL,INFO.USER_FIRST_NAME,INFO.USER_MIDDLE_NAME,INFO.USER_LAST_NAME,INFO.USER_PHONE_NO,ROLEMAP.USER_ROLE,ROLEMAP.PARTNER_ID,CARD.ACCOUNT_ID,(CARD.cardgroup_main_type||CARD.cardgroup_sub_type||CARD.cardgroup_seq) as "CARD_GROUP",CARD.PRT_CARD_PK as CARD,CARD.CARD_EMBOSS_NUM as CARDEMBOSSNO
FROM PRT_CARD_USER_ROLE_MAPPING ROLEMAP  LEFT JOIN PRT_CARD_USER_INFORMATION INFO 
ON (INFO.USER_EMAIL = ROLEMAP.USER_EMAIL AND INFO.COUNTRY_CODE = ROLEMAP.COUNTRY_CODE) 
LEFT JOIN PRT_CARD CARD
ON (CARD.PRT_CARD_PK = ROLEMAP.USER_ASSOCIATION)
WHERE INFO.COUNTRY_CODE = :cc AND ROLEMAP.USER_ROLE = :roleName AND INSTR(:PID,ROLEMAP.PARTNER_ID)<>0
AND ROLEMAP.ASSOCIATION_TYPE = 'CC']]>
  </SQLQuery>
