  <SQLQuery>
    <![CDATA[select * from
(SELECT PRT_CARD_TRANSACTION_HEADER.PARTNER_ID,PRT_CARD_TRANSACTION_HEADER.KSID,PRT_CARD_TRANSACTION_HEADER.TRANSACTION_TIME,PRT_CARD_TRANSACTION_HEADER.TRANSACTION_DT,PRT_CARD_TRANSACTION_HEADER.transaction_type,PRT_CARD_TRANSACTION_HEADER.card_1_id,PRT_CARD_TRANSACTION_HEADER.station_name,PRT_CARD_TRANSACTION_DETAIL.PRODUCT_NAME,PRT_CARD_TRANSACTION_DETAIL.currency_gross_amount,PRT_CARD_TRANSACTION_DETAIL.Quantity,PRT_CARD_TRANSACTION_DETAIL.UNIT_OF_MEASURE FROM PRT_CARD_TRANSACTION_DETAIL
inner join PRT_CARD_TRANSACTION_HEADER
ON (PRT_CARD_TRANSACTION_HEADER.UREF_TRANSACTION_ID=PRT_CARD_TRANSACTION_DETAIL.UREF_TRANSACTION_ID
and PRT_CARD_TRANSACTION_HEADER.pals_country_code=PRT_CARD_TRANSACTION_DETAIL.pals_country_code)
where PRT_CARD_TRANSACTION_DETAIL.pals_country_code =:countryCode
and INSTR(:partnerId,PARTNER_ID)<>0
AND PRT_CARD_TRANSACTION_DETAIL.PRICE_ELEMENT_CODE IS NULL
ORDER by PRT_CARD_TRANSACTION_HEADER.TRANSACTION_TIME DESC)
where ROWNUM <=5]]>
