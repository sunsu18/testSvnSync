  <SQLQuery>
    <![CDATA[select decode(prt_card_invoice.collective_invoice_number,null,prt_card_invoice.invoice_number,prt_card_invoice.collective_invoice_number) as finalInvoice , prt_card_invoice.invoice_number,prt_card_invoice.collective_invoice_number,prt_card_invoice_details.INVOICE_DOC_TYPE, 
prt_card_invoice.invoice_date Invoice_Date,prt_card_payment_doc.invoicing_due_date Due_Date,prt_card_invoice_details.inv_gross_amt Gross,prt_card_invoice_details.inv_vat_amt VAT,prt_card_invoice_details.invoice_currency,
prt_card_payment_doc.cardgroup_main_type,prt_card_payment_doc.cardgroup_sub_type,prt_card_payment_doc.cardgroup_seq,
prt_card_invoice.invoiced_card
from prt_card_invoice join prt_card_payment_doc ON 

( (prt_card_payment_doc.invoice_doc_number = decode(prt_card_invoice.collective_invoice_number,null,prt_card_invoice.invoice_number,prt_card_invoice.collective_invoice_number)
or
prt_card_invoice.invoice_number like '1%'
) and prt_card_payment_doc.country_code=prt_card_invoice.country_code)

left join prt_card_invoice_details on ( prt_card_invoice.collective_invoice_number = prt_card_invoice_details.invoice_doc_number and prt_card_invoice. country_code = prt_card_invoice_details. country_code)
Where prt_card_payment_doc.COUNTRY_CODE = :countryCode AND
prt_card_payment_doc.PARTNER_ID =:partnerId AND INSTR(:accountId,prt_card_payment_doc.ACCOUNT_ID) <> 0 
AND prt_card_payment_doc.INVOICING_DATE >=: fromDateBV AND prt_card_payment_doc.INVOICING_DATE <=: toDateBV]]>
  </SQLQuery>
