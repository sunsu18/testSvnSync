<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PrtViewVehicleDriverVO"
  Version="11.1.1.61.92"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false"
  RowClass="com.sfr.engage.model.queries.uvo.PrtViewVehicleDriverVORowImpl"
  ComponentClass="com.sfr.engage.model.queries.uvo.PrtViewVehicleDriverVOImpl">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|Prog|VarAccess"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.sfr.engage.model.queries.uvo.PrtViewVehicleDriverVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="countryCd"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="paramValue"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="accountValue"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT PDI.DRIVER_NAME AS DRIVER_NAME
       ,PDI.DRIVER_NUMBER AS DRIVER_NUMBER
       ,null as VEHICLE_NUMBER
       ,null as INTERNAL_NAME
     
FROM PRT_DRIVER_INFORMATION PDI
WHERE  INSTR(:accountValue,PDI.ACCOUNT_NUMBER)<>0
AND   PDI.COUNTRY_CODE       = :countryCd
AND   'Driver'               = :paramValue

UNION

SELECT null as DRIVER_NAME 
       ,null as DRIVER_NUMBER
       ,PTI.VEHICLE_NUMBER
       ,PTI.INTERNAL_NAME
      

FROM PRT_TRUCK_INFORMATION PTI
WHERE  INSTR(:accountValue,PTI.ACCOUNT_NUMBER)<>0
AND   PTI.COUNTRY_CODE       = :countryCd
AND   'Vehicle'               = :paramValue]]>
  </SQLQuery>
  <ViewAttribute
    Name="DriverName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DRIVER_NAME"
    Expression="DRIVER_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DriverNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DRIVER_NUMBER"
    Expression="DRIVER_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="VehicleNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VEHICLE_NUMBER"
    Expression="VEHICLE_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="InternalName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INTERNAL_NAME"
    Expression="INTERNAL_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.sfr.engage.model.EngageModelBundle"/>
  </ResourceBundle>
</ViewObject>
