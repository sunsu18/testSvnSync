                             binding="#{pageFlowScope.PriceListBean.keycode}">
                --><!--<af:serverListener type="myCustomEvent"
                                   method="#{pageFlowScope.PriceListBean.mapvalue}"/>--><!--
                <af:clientListener method="seevalue" type="action"/>
              </af:outputText>
            </af:panelGroupLayout>
          </af:column>
          --><!--<af:column sortProperty="PricingUom" sortable="false" headerText="#{bindings.PriceListRVO1.hints.PricingUom.label}" id="c2">
          <af:outputText value="#{row.PricingUom}" id="ot5"/>
        </af:column>--><!--
        </af:table>-->
      </af:panelGroupLayout>
      <af:spacer width="10" height="10" id="s8"/>
    </af:panelGroupLayout>
  </af:panelGroupLayout>
</jsp:root>
