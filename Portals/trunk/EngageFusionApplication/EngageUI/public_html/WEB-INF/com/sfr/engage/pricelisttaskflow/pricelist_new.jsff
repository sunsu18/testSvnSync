        <af:spacer width="62" height="10" id="s5"/>
        <af:outputText value="#{bindings.ModifiedDate.inputValue}" id="ot8"
                       >
          <af:convertDateTime pattern="dd-MMM-yyyy  HH:mm:ss"/>
        </af:outputText>
        <!--<af:outputText value="#{bindings.ModifiedDate.inputValue}" id="ot6">
          <af:convertDateTime pattern="dd - MMM - yyyy HH:mm:ss"/>
        </af:outputText>-->
      </af:panelGroupLayout>
      <af:spacer width="10" height="10" id="s7"/>
      <af:panelGroupLayout id="pgl3" layout="horizontal">
        <!--<af:spacer width="10" height="10" id="s7"/>-->
        <af:table value="#{bindings.PriceListNewRVO11.collectionModel}"
                  var="row" rows="#{bindings.PriceListNewRVO11.rangeSize}"
                  emptyText="#{bindings.PriceListNewRVO11.viewable ? 'No data to display.' : 'Access Denied.'}"
                  fetchSize="#{bindings.PriceListNewRVO11.rangeSize}"
                  rowBandingInterval="1" id="t1"
                  binding="#{pageFlowScope.PriceList_newBean.bindings.pricelisttable}" >
          <af:column sortProperty="DescLine1" sortable="false"
                     headerText="#{sfr.PRODUCTS}" width="145"
                     id="c2">
                      <af:panelGroupLayout id="pgl4" layout="horizontal" halign="left" styleClass="tablepanel">
            <af:outputText value="#{row.DescLine1}" id="ot5"/>
            </af:panelGroupLayout>
          </af:column>
          <af:column sortProperty="UnitPrice" sortable="false"
                     headerText="#{sfr.UNIT_PRICE}"
                     id="c1" width="145">
                     <af:panelGroupLayout id="pgl0" layout="horizontal" halign="right" 
                                 styleClass="tablepanel">
            <af:outputText value="#{row.UnitPrice}" id="ot3">
              <af:convertNumber groupingUsed="true"
                                locale="#{pageFlowScope.PriceList_newBean.locale}"
                                pattern="##.##"/>
            </af:outputText>
             <af:spacer width="10" height="1" id="s2"/>
              <af:outputText value="#{row.CurrencyCode}" id="ot6"/>
              <af:outputText value="./" id="ot7"/>
               <af:outputText value="#{row.Uom}" id="ot2"/>
            </af:panelGroupLayout>
          </af:column>
        
        </af:table>
