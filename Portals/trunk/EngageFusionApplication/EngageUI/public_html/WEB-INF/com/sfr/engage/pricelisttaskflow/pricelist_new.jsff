                  rowBandingInterval="1" id="t1" binding="#{pageFlowScope.PriceListBean.priceTable}" summary="pricelist table">
          <af:column sortProperty="DescLine1" sortable="false" headerText="#{sfr.PRODUCTS}" id="c3" width="145" inlineStyle="padding-left:10px;">
          <af:panelGroupLayout id="pgl4" layout="horizontal" halign="left">
  
 <af:outputText value="#{row.DescLine1}" id="ot3"/></af:panelGroupLayout> 
          </af:column>
          <af:column sortProperty="UnitPrice" sortable="false" headerText="#{sfr.UNIT_PRICE}" id="c1" align="right" width="145"
                     >
            <af:panelGroupLayout id="pgl0" layout="horizontal" halign="right" 
                                 styleClass="tablepanel">
              <af:outputText value="#{row.UnitPrice}" id="ot2">
                <af:convertNumber groupingUsed="true" locale="#{pageFlowScope.PriceListBean.locale}"/>
              </af:outputText>
              <af:spacer width="10" height="1" id="s2"/>
              <af:outputText value="  kr./" id="ot5"/>
