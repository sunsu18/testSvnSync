  Oracle JDeveloper BPEL Designer 
  
  Created: Wed Feb 19 18:26:34 IST 2014
  Author:  10608609
  Type: BPEL 1.1 Process
  Purpose: Synchronous BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="E001PortalBalanceCheckBPEL"
               targetNamespace="http://xmlns.oracle.com/E001SyncPortalBalanceChecktoCGIApp/E001SyncPortalBalanceChecktoCGI/E001PortalBalanceCheckBPEL"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/E001SyncPortalBalanceChecktoCGIApp/E001SyncPortalBalanceChecktoCGI/E001PortalBalanceCheckBPEL"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://www.logica.no/card/sms/soap"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns2="http://www.statoilfuelretail.com/integration/E001/E001PortalBalanceCheck"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:aia="http://www.oracle.com/XSL/Transform/java/oracle.apps.aia.core.xpath.AIAFunctions"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns3="http://xmlns.oracle.com/ias/pcbpel/NotificationService"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns4="http://www.statoilfuelretail.com/integration/E001/E001InstanceId"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/jms/E001SyncPortalBalanceChecktoCGIApp/E001SyncPortalBalanceChecktoCGI/SOA_ErrorInfo_JMSQ"
         xmlns:ns6="http://www.statoilfuelretail.com/integration/E001/E001PortalBalanceCheckling.xsd"
         xmlns:ns7="http://www.statoilfuelretail.com/integration/engage/ErrorHandling.xsd">

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="e001portalbalancecheckbpel_client" partnerLinkType="client:E001PortalBalanceCheckBPEL" myRole="E001PortalBalanceCheckBPELProvider"/>
    <partnerLink name="CGIWebServiceV1"
                 partnerLinkType="ns1:InvokeCGIWebServiceV1" partnerRole="sms"/>
    <partnerLink name="SOA_ErrorInfo_JMSQ"
                 partnerLinkType="ns5:Produce_Message_plt"
                 partnerRole="Produce_Message_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:E001PortalBalanceCheckBPELRequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:E001PortalBalanceCheckBPELResponseMessage"/>
    <variable name="InvokeCGIWebServiceCallaccountbalance_account-balance_IV"
              messageType="ns1:account-balance-request"/>
    <variable name="InvokeCGIWebServiceCallaccountbalance_account-balance_OV"
              messageType="ns1:account-balance-response"/>
    <variable name="remoteFaultMsg" type="xsd:string"/>
    <variable name="instanceId" element="ns4:rootNode"/>
    <variable name="InstanceID" type="xsd:string"/>
    <variable name="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
              messageType="ns5:Produce_Message_msg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <bpelx:annotation>
        <bpelx:documentation>For all system faults</bpelx:documentation>
      </bpelx:annotation>
      <sequence>
        <assign name="AssignOutputfaultVariable">
          <copy>
            <from expression="'E002'"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:errorCode"/>
          </copy>
          <copy>
            <from expression="'Request has been timed out due to technical error.'"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:errorMessage"/>
          </copy>
          <copy>
            <from expression="ora:getCompositeInstanceId()"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:instanceID"/>
          </copy>
        </assign>
        <reply name="ReplyRemoteFault" variable="outputVariable"
               partnerLink="e001portalbalancecheckbpel_client"
               portType="client:E001PortalBalanceCheckBPEL"
               operation="process"/>
        <assign name="AssignCommonErrorHandlerMessage">
          <copy>
            <from expression="ora:getCompositeInstanceId()"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:CommonErrorData/ns7:InstanceID"/>
          </copy>
          <copy>
            <from expression="ora:getCompositeName()"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:CommonErrorData/ns7:ComponentName"/>
          </copy>
          <copy>
            <from expression="'E001'"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:CommonErrorData/ns7:ProjectID"/>
          </copy>
          <copy>
            <from expression="'E001SyncPortalBalanceChecktoCGI'"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:CommonErrorData/ns7:ProjectName"/>
          </copy>
          <copy>
            <from expression="xp20:current-dateTime()"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:CommonErrorData/ns7:Timestamp"/>
          </copy>
          <copy>
            <from expression="'Portal'"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:CommonErrorData/ns7:Source"/>
          </copy>
          <copy>
            <from expression="'CGI'"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:CommonErrorData/ns7:Target"/>
          </copy>
          <copy>
            <from variable="inputVariable" part="payload"
                  query="/ns2:requestPayload/ns2:userID"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:CommonErrorData/ns7:UserID"/>
          </copy>
          <copy>
            <from expression="'TE-001'"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:ErrorDetails/ns7:ErrorCode"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:ErrorDetails/ns7:ErrorDescription"/>
          </copy>
          <copy>
            <from expression="'E001_TE'"/>
            <to variable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                part="body"
                query="/ns7:ErrorInfo/ns7:ErrorNotification/ns7:LookupCriteria"/>
          </copy>
        </assign>
        <invoke name="InvokeSOA_ErrorInfo_JMSQ"
                inputVariable="InvokeSOA_ErrorInfo_JMSQ_Produce_Message_IV"
                partnerLink="SOA_ErrorInfo_JMSQ"
                portType="ns5:Produce_Message_ptt" operation="Produce_Message"
                bpelx:invokeAsDetail="no"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in E001PortalBalanceCheckBPEL.wsdl) -->
    <receive name="receiveInput" partnerLink="e001portalbalancecheckbpel_client" portType="client:E001PortalBalanceCheckBPEL" operation="process" variable="inputVariable" createInstance="yes"/>
    <scope name="ValidateInputVariableScope" variableAccessSerializable="no">
      <variables>
        <variable name="errorPayload"
                  messageType="client:E001PortalBalanceCheckBPELResponseMessage"/>
      </variables>
      <faultHandlers>
        <catch faultName="bpelx:invalidVariables">
          <sequence>
            <assign name="AssigninstanceId">
              <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="instanceId" query="/ns4:rootNode/ns4:instanceId"/>
              </copy>
            </assign>
            <assign name="XformInputVariableToErrorPayload">
              <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
              </bpelx:annotation>
              <copy>
                <from expression="ora:doXSLTransformForDoc('xsl/E001PortalBalanceCheck_to_PortalBalanceCheckError.xsl', $inputVariable.payload, 'instanceId', $instanceId)"/>
                <to variable="errorPayload" part="payload"/>
              </copy>
            </assign>
            <reply name="ReplyErrorPayload" variable="errorPayload"
                   partnerLink="e001portalbalancecheckbpel_client"
                   portType="client:E001PortalBalanceCheckBPEL"
                   operation="process"/>
            <terminate name="TerminateProcess"/>
          </sequence>
        </catch>
      </faultHandlers>
      <sequence name="Sequence2">
        <bpelx:validate name="ValidateInputVariable" variables="inputVariable"/>
      </sequence>
    </scope>
    <assign name="AssignInputVariable">
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:requestPayload/ns2:cardNumber"/>
        <to variable="InvokeCGIWebServiceCallaccountbalance_account-balance_IV"
            part="account-balance-request"
            query="/ns1:account-balance-request/cardNumber"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:requestPayload/ns2:mobilePhone"/>
        <to variable="InvokeCGIWebServiceCallaccountbalance_account-balance_IV"
            part="account-balance-request"
            query="/ns1:account-balance-request/mobilePhone"/>
      </copy>
    </assign>
    <invoke name="InvokeCGIWebServiceCallAccountBalance"
            inputVariable="InvokeCGIWebServiceCallaccountbalance_account-balance_IV"
            outputVariable="InvokeCGIWebServiceCallaccountbalance_account-balance_OV"
            partnerLink="CGIWebServiceV1" portType="ns1:sms"
            operation="account-balance" bpelx:invokeAsDetail="no"/>
    <switch name="Switch1">
      <case condition="bpws:getVariableData('InvokeCGIWebServiceCallaccountbalance_account-balance_OV','account-balance-response','/ns1:account-balance-response/state')='ERROR'">
        <bpelx:annotation>
          <bpelx:pattern>CardNUmber entered by the user does not exist</bpelx:pattern>
          <bpelx:general>
            <bpelx:property name="userLabel">NonExistantCardNumber</bpelx:property>
          </bpelx:general>
        </bpelx:annotation>
        <assign name="AssignOutputVariableError">
          <copy>
            <from expression="'E101'"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:errorCode"/>
          </copy>
          <copy>
            <from expression="ora:getCompositeInstanceId()"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:instanceID"/>
          </copy>
          <copy>
            <from variable="InvokeCGIWebServiceCallaccountbalance_account-balance_OV"
                  part="account-balance-response"
                  query="/ns1:account-balance-response/state"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:state"/>
          </copy>
          <copy>
            <from expression="'Invalid card or Mobile number'"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:errorMessage"/>
          </copy>
        </assign>
      </case>
      <otherwise>
        <assign name="AssignOutputVariable">
          <copy>
            <from variable="InvokeCGIWebServiceCallaccountbalance_account-balance_OV"
                  part="account-balance-response"
                  query="/ns1:account-balance-response/balance"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:balance"/>
          </copy>
          <copy>
            <from expression="ora:getCompositeInstanceId()"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:instanceID"/>
          </copy>
          <copy>
            <from variable="InvokeCGIWebServiceCallaccountbalance_account-balance_OV"
                  part="account-balance-response"
                  query="/ns1:account-balance-response/currencyCode"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:currencyCode"/>
          </copy>
          <copy>
            <from variable="InvokeCGIWebServiceCallaccountbalance_account-balance_OV"
                  part="account-balance-response"
                  query="/ns1:account-balance-response/mobilePhone"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:mobilePhone"/>
          </copy>
          <copy>
            <from variable="InvokeCGIWebServiceCallaccountbalance_account-balance_OV"
                  part="account-balance-response"
                  query="/ns1:account-balance-response/state"/>
            <to variable="outputVariable" part="payload"
                query="/ns2:responsePayload/ns2:state"/>
          </copy>
        </assign>
      </otherwise>
    </switch>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" partnerLink="e001portalbalancecheckbpel_client" portType="client:E001PortalBalanceCheckBPEL" operation="process" variable="outputVariable"/>
  </sequence>
</process>