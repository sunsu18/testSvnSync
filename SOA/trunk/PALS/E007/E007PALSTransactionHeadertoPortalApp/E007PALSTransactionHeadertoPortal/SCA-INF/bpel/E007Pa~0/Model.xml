          </sequence>
        </scope>
        <assign name="NextBatch" id="BpAss17">
          <copy>
            <from expression="bpws:getVariableData('batchCountIterator') + 1"/>
            <to variable="batchCountIterator"/>
          </copy>
        </assign>
      </sequence>
    </while>
  </sequence>
