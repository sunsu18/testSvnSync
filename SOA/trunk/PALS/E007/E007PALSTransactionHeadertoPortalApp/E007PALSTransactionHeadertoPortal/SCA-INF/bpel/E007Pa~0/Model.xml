                </assign>
                <invoke name="InvokeMergeTxnHeaderDataPortalDBAdapterV1" inputVariable="InvokeMergeTxnHeaderDataPortalDBAdapterV1_IV" partnerLink="MergeTxnHeaderPortalDBAdapterV1" portType="ns11:MergeTxnHeaderPortalDBAdapterV1_ptt" operation="merge" bpelx:invokeAsDetail="no" id="BpInv5"/>
              </sequence>
