<process name="E002JDEE1toWebFuelBPEL"
               targetNamespace="http://www.statoilfuelretail.com/integration/E002/E002JDEE1toWebFuelApp/E002JDEE1toWebFuel/E002JDEE1toWebFuelBPEL"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://www.statoilfuelretail.com/integration/E002/E002JDEE1toWebFuelApp/E002JDEE1toWebFuel/E002JDEE1toWebFuelBPEL"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/E002JDEE1toWebFuelApp/E002JDEE1toWebFuel/MergeWebFuelDBAdapterV1"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/E002JDEE1toWebFuelApp/E002JDEE1toWebFuel/CraeteJDEE1DBAdapterV1"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/E002JDEE1toWebFuelApp/E002JDEE1toWebFuel/SelectJDEE1DBAdaptorV1"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:ns4="http://www.statoilfuelretail.com/integration/E002/E002InvocationReqMsg">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="E002JDEE1toWebFuelbpel_client" partnerLinkType="client:E002JDEE1toWebFuelBPEL" myRole="E002JDEE1toWebFuelBPELProvider"/>
    <partnerLink name="MergeWebFuelDBAdapterV1"
                 partnerLinkType="ns1:MergeWebFuelDBAdapterV1_plt"
                 partnerRole="MergeWebFuelDBAdapterV1_role"/>
    <partnerLink name="SelectJDEE1DBAdaptorV1"
                 partnerLinkType="ns3:SelectJDEE1DBAdaptorV1_plt"
                 partnerRole="SelectJDEE1DBAdaptorV1_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:E002JDEE1toWebFuelBPELRequestMessage"/>
    <variable name="InvokeJDEE1DBAdaptorV1Select_IV"
              messageType="ns3:SelectJDEE1DBAdaptorV1Select_inputParameters"/>
    <variable name="InvokeJDEE1DBAdaptorV1Select_OV"
              messageType="ns3:F5542112Collection_msg"/>
    <variable name="InvokeWebFuelMerge_IV"
              messageType="ns1:PrtListPricesCollection_msg"/>
    <variable name="UserName" type="xsd:string"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in E002JDEE1toWebFuelBPEL.wsdl) -->
    <receive name="receiveInput" partnerLink="E002JDEE1toWebFuelbpel_client" portType="client:E002JDEE1toWebFuelBPEL" operation="process" variable="inputVariable" createInstance="yes"/>
    <assign name="AssignUserName">
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns4:invocationMsg/ns4:UserName"/>
        <to variable="UserName"/>
      </copy>
    </assign>
    <invoke name="InvokeSelectJDEE1DBAdaptorV1"
            inputVariable="InvokeJDEE1DBAdaptorV1Select_IV"
            partnerLink="SelectJDEE1DBAdaptorV1"
            portType="ns3:SelectJDEE1DBAdaptorV1_ptt"
            operation="SelectJDEE1DBAdaptorV1Select"
            outputVariable="InvokeJDEE1DBAdaptorV1Select_OV"
            bpelx:invokeAsDetail="no"/>
    <assign name="xformInvokeJDEE1toMergeWebFuel">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('xsl/JDEE1DBAdaptorV1Select_OVtoMergeWebFuel_IV.xsl', $InvokeJDEE1DBAdaptorV1Select_OV.F5542112Collection)"/>
        <to variable="InvokeWebFuelMerge_IV" part="PrtListPricesCollection"/>
      </copy>
    </assign>
    <invoke name="InvokeMergeWebFuelDBAdaptorV1"
            inputVariable="InvokeWebFuelMerge_IV"
            partnerLink="MergeWebFuelDBAdapterV1"
            portType="ns1:MergeWebFuelDBAdapterV1_ptt" operation="merge"
            bpelx:invokeAsDetail="no"/>
  </sequence>
</process>