    <variable name="txnReport" element="ns1:TransactionReport"/>
    <variable name="transactionReportInput"
              element="ns1:TransactionReportInput"/>
    <variable name="InvokeWriteTransactionReport_IV"
              messageType="ns2:Write_msg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <empty name="Empty"/>
    </catchAll>
  </faultHandlers>
